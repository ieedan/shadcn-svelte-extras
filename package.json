{
	"name": "shadcn-svelte-extras",
	"packageManager": "pnpm@10.23.0",
	"version": "6.8.2",
	"type": "module",
	"scripts": {
		"dev": "concurrently -n 'registry,app' -c 'blue,green' \"pnpm registry:dev\"  \"pnpm generate-llms && vite dev\"",
		"build": "pnpm pre:build && vite build",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"registry:build": "jsrepo build",
		"registry:dev": "jsrepo build --watch",
		"ci:release": "jsrepo publish && changeset tag",
		"generate-i18n": "pnpm paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"generate-llms": "tsx scripts/generate-llms.ts",
		"pre:build": "pnpm sync && pnpm generate-llms && pnpm registry:build"
	},
	"devDependencies": {
		"@changesets/cli": "latest",
		"@emoji-mart/data": "^1.2.1",
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@fontsource-variable/geist": "^5.2.8",
		"@fontsource-variable/geist-mono": "^5.2.7",
		"@inlang/cli": "^3.1.6",
		"@inlang/paraglide-js": "2.13.0",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.575.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@shikijs/langs": "^4.0.0",
		"@shikijs/themes": "^4.0.0",
		"@sveltejs/adapter-vercel": "^6.3.3",
		"@sveltejs/kit": "^2.53.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.2.1",
		"@types/node": "^25.3.2",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"bits-ui": "^2.16.2",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"country-flag-icons": "^1.6.15",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"isomorphic-dompurify": "2.25.0",
		"jsrepo": "3.6.1",
		"mode-watcher": "^1.1.0",
		"package-manager-detector": "^1.6.0",
		"paneforge": "^1.0.2",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"runed": "^0.37.1",
		"shiki": "^4.0.0",
		"svelte": "^5.53.5",
		"svelte-check": "^4.4.4",
		"svelte-easy-crop": "^5.0.0",
		"svelte-meta-tags": "^4.5.0",
		"svelte-sonner": "^1.0.7",
		"svelte-tel-input": "^3.6.1",
		"svelte-toolbelt": "^0.10.6",
		"sveltekit-superforms": "^2.30.0",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.2.1",
		"tsx": "^4.21.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1",
		"valibot": "1.2.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "7.3.1",
		"vite-plugin-transform-lucide-imports": "^0.3.0",
		"yeezy-dates": "^1.1.0"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"dependencies": {
		"@svitejs/changesets-changelog-github-compact": "^1.2.0"
	}
}
