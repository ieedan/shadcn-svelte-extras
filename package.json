{
	"name": "shadcn-svelte-extras",
	"packageManager": "pnpm@10.11.1",
	"version": "3.4.0",
	"type": "module",
	"scripts": {
		"dev": "pnpm generate-reference && vite dev",
		"build": "pnpm generate-reference && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"build:registry": "jsrepo build",
		"ci:release": "pnpm jsrepo publish --verbose && changeset tag",
		"generate-i18n": "pnpm paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"generate-reference": "pnpm dlx tsx ./src/lib/scripts/generate-reference.ts"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.4",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@fontsource-variable/geist-mono": "^5.2.6",
		"@fontsource-variable/inter": "^5.2.5",
		"@inlang/cli": "^3.0.11",
		"@inlang/paraglide-js": "2.0.13",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.513.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@shikijs/langs": "^3.5.0",
		"@shikijs/themes": "^3.5.0",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.21.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.7",
		"bits-ui": "2.4.1",
		"clsx": "^2.1.1",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"globals": "^16.2.0",
		"isomorphic-dompurify": "^2.25.0",
		"mode-watcher": "^1.0.7",
		"package-manager-detector": "^1.3.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"runed": "^0.28.0",
		"shiki": "^3.5.0",
		"svelte": "^5.33.14",
		"svelte-check": "^4.2.1",
		"svelte-easy-crop": "^4.0.1",
		"svelte-meta-tags": "^4.4.0",
		"svelte-sonner": "^1.0.4",
		"svelte-tel-input": "^3.6.0",
		"svelte-toolbelt": "^0.9.1",
		"sveltekit-superforms": "^2.26.0",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.8",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1",
		"valibot": "1.1.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^6.3.5",
		"yeezy-dates": "^1.0.1"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"dependencies": {
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"jsrepo": "^2.3.3"
	}
}
