{
	"name": "shadcn-svelte-extras",
	"packageManager": "pnpm@10.11.1",
	"version": "5.3.1",
	"type": "module",
	"scripts": {
		"dev": "pnpm generate-llms && vite dev",
		"build": "pnpm generate-llms && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"build:registry": "jsrepo build",
		"ci:release": "pnpm jsrepo publish --verbose && changeset tag",
		"generate-i18n": "pnpm paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"generate-llms": "pnpm sync && pnpm dlx tsx ./src/lib/scripts/generate-llms.ts"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.5",
		"@emoji-mart/data": "^1.2.1",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@fontsource-variable/geist": "^5.2.6",
		"@fontsource-variable/geist-mono": "^5.2.6",
		"@inlang/cli": "^3.0.12",
		"@inlang/paraglide-js": "2.1.0",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.525.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@shikijs/langs": "^3.8.1",
		"@shikijs/themes": "^3.8.1",
		"@sveltejs/adapter-vercel": "^5.8.1",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/vite": "^4.1.11",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"bits-ui": "^2.8.13",
		"clsx": "^2.1.1",
		"country-flag-icons": "^1.5.19",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"isomorphic-dompurify": "^2.26.0",
		"mode-watcher": "^1.1.0",
		"package-manager-detector": "^1.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"runed": "^0.29.2",
		"shiki": "^3.8.1",
		"svelte": "^5.37.0",
		"svelte-check": "^4.3.0",
		"svelte-easy-crop": "^4.0.1",
		"svelte-meta-tags": "^4.4.0",
		"svelte-sonner": "^1.0.5",
		"svelte-tel-input": "^3.6.0",
		"svelte-toolbelt": "^0.9.3",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.11",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"valibot": "1.1.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^7.0.6",
		"yeezy-dates": "^1.0.1"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"dependencies": {
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"jsrepo": "^2.4.3"
	}
}
